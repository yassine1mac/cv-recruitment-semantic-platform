@prefix : <http://www.semanticweb.org/ontologies/cv#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://www.semanticweb.org/ontologies/cv> rdf:type owl:Ontology ;
    rdfs:label "CV Ontology"@en ;
    rdfs:comment "Ontologie pour la modélisation de CV et la sélection de candidats"@fr .

#################################################################
#    Classes
#################################################################

:Person rdf:type owl:Class ;
    rdfs:label "Person"@en , "Personne"@fr ;
    rdfs:comment "Une personne candidate à un poste"@fr .

:Skill rdf:type owl:Class ;
    rdfs:label "Skill"@en , "Compétence"@fr ;
    rdfs:comment "Une compétence technique ou transversale"@fr .

:TechnicalSkill rdf:type owl:Class ;
    rdfs:subClassOf :Skill ;
    rdfs:label "Technical Skill"@en , "Compétence Technique"@fr .

:SoftSkill rdf:type owl:Class ;
    rdfs:subClassOf :Skill ;
    rdfs:label "Soft Skill"@en , "Compétence Transversale"@fr .

:Degree rdf:type owl:Class ;
    rdfs:label "Degree"@en , "Diplôme"@fr ;
    rdfs:comment "Un diplôme ou certification"@fr .

:Experience rdf:type owl:Class ;
    rdfs:label "Experience"@en , "Expérience"@fr ;
    rdfs:comment "Une expérience professionnelle"@fr .

:Profile rdf:type owl:Class ;
    rdfs:label "Profile"@en , "Profil"@fr ;
    rdfs:comment "Un profil professionnel (DataScientist, Developer, etc.)"@fr .

:DataScientist rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Data Scientist"@en .

:WebDeveloper rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Web Developer"@en , "Développeur Web"@fr .

:BackendDeveloper rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Backend Developer"@en .

:FullStackDeveloper rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Full Stack Developer"@en .

:DataEngineer rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Data Engineer"@en , "Ingénieur Data"@fr .

:MLEngineer rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "ML Engineer"@en , "Ingénieur Machine Learning"@fr .

:DevOpsEngineer rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "DevOps Engineer"@en .

:FrontendDeveloper rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Frontend Developer"@en , "Développeur Frontend"@fr .

:MobileDeveloper rdf:type :Profile ;
    rdfs:label "Mobile Developer"@en , "Développeur Mobile"@fr .

:CyberSecurityAnalyst rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Cybersecurity Analyst"@en , "Analyste Cybersécurité"@fr .

:CloudEngineer rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Cloud Engineer"@en , "Ingénieur Cloud"@fr .

:DatabaseAdministrator rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Database Administrator"@en , "Administrateur Base de Données"@fr .

:BusinessAnalyst rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Business Analyst"@en , "Analyste Métier"@fr .

:ProjectManager rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "Project Manager"@en , "Chef de Projet"@fr .

:QAEngineer rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "QA Engineer"@en , "Ingénieur QA"@fr .

:UIUXDesigner rdf:type owl:NamedIndividual , :Profile ;
    rdfs:label "UI/UX Designer"@en , "Designer UI/UX"@fr .




#################################################################
#    Object Properties
#################################################################

:hasSkill rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Skill ;
    rdfs:label "has skill"@en , "possède compétence"@fr .

:hasDegree rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Degree ;
    rdfs:label "has degree"@en , "possède diplôme"@fr .

:hasExperience rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Experience ;
    rdfs:label "has experience"@en , "possède expérience"@fr .

:hasProfile rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Profile ;
    rdfs:label "has profile"@en , "a profil"@fr .

:workedAt rdf:type owl:ObjectProperty ;
    rdfs:domain :Experience ;
    rdfs:label "worked at"@en , "a travaillé chez"@fr .

#################################################################
#    Data Properties
#################################################################

:name rdf:type owl:DatatypeProperty ;
    rdfs:domain :Person ;
    rdfs:range xsd:string ;
    rdfs:label "name"@en , "nom"@fr .

:email rdf:type owl:DatatypeProperty ;
    rdfs:domain :Person ;
    rdfs:range xsd:string ;
    rdfs:label "email"@en .

:skillName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Skill ;
    rdfs:range xsd:string ;
    rdfs:label "skill name"@en , "nom compétence"@fr .

:skillLevel rdf:type owl:DatatypeProperty ;
    rdfs:domain :Skill ;
    rdfs:range xsd:string ;
    rdfs:comment "Niveau: Débutant, Intermédiaire, Avancé, Expert"@fr .

:degreeName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Degree ;
    rdfs:range xsd:string ;
    rdfs:label "degree name"@en , "nom diplôme"@fr .

:degreeLevel rdf:type owl:DatatypeProperty ;
    rdfs:domain :Degree ;
    rdfs:range xsd:string ;
    rdfs:comment "Niveau: Bac, Bac+2, Bac+3, Bac+5, Doctorat"@fr .

:yearObtained rdf:type owl:DatatypeProperty ;
    rdfs:domain :Degree ;
    rdfs:range xsd:integer ;
    rdfs:label "year obtained"@en , "année obtention"@fr .

:jobTitle rdf:type owl:DatatypeProperty ;
    rdfs:domain :Experience ;
    rdfs:range xsd:string ;
    rdfs:label "job title"@en , "poste"@fr .

:company rdf:type owl:DatatypeProperty ;
    rdfs:domain :Experience ;
    rdfs:range xsd:string ;
    rdfs:label "company"@en , "entreprise"@fr .

:duration rdf:type owl:DatatypeProperty ;
    rdfs:domain :Experience ;
    rdfs:range xsd:integer ;
    rdfs:comment "Durée en mois"@fr .

:startYear rdf:type owl:DatatypeProperty ;
    rdfs:domain :Experience ;
    rdfs:range xsd:integer .

:endYear rdf:type owl:DatatypeProperty ;
    rdfs:domain :Experience ;
    rdfs:range xsd:integer .

:yearsOfExperience rdf:type owl:DatatypeProperty ;
    rdfs:domain :Person ;
    rdfs:range xsd:integer ;
    rdfs:label "years of experience"@en , "années d'expérience"@fr .

